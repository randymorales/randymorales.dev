    "export": "next export",
    "start": "next start"
  },
  "dependencies": {
    "date-fns": "^2.16.1",
    "esm": "^3.2.25",
    "gray-matter": "^4.0.2",
    "next": "^10.0.7",
    "prismjs": "^1.24.1",
    "react": "^17",
    "react-dom": "^17",
    "react-transition-group": "^4.4.1",
    "remark": "^13",
    "remark-html": "^13",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "prettier": "2.2.1",
    "husky": "^4.2.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run cache-posts && git add cache/data.js"
